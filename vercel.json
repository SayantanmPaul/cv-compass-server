{
    "builds": [
        {
            "src": "src/app.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.ts"
        }
    ],
    "functions": {
        "src/app.ts": {
            "runtime": "@vercel/node"
        }
    }
}